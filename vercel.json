{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "rewrites": [
      {
        "source": "/assets/files/:file",
        "destination": "/assets/files/:file"
      }
    ]
  }
  