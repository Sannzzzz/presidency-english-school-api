{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "rewrites": [
      {
        "source": "/assets/files/:Yearly_Plan_2025_26.pdf",
        "destination": "/assets/files/:Yearly_Plan_2025_26.pdf"
      }
    ]
  }
  